{
  "name": "@keyvalue/client",
  "version": "0.1.0",
  "description": "TypeScript/JavaScript client for Key-Value store with memorable tokens",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "key-value",
    "kv",
    "storage",
    "memorable-tokens",
    "typescript",
    "api-client",
    "time-series",
    "iot",
    "edge"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mikro-design/key-value.js"
  },
  "bugs": {
    "url": "https://github.com/mikro-design/key-value.js/issues"
  },
  "homepage": "https://github.com/mikro-design/key-value.js#readme",
  "devDependencies": {
    "@types/node": "^20.19.24",
    "tsup": "^8.5.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
